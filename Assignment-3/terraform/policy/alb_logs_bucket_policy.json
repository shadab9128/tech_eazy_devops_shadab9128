{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowALBLogging",
      "Effect": "Allow",
      "Principal": {
        "Service": "logdelivery.elasticloadbalancing.amazonaws.com"
      },
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::alb_logs_bucket_name/*",
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "435917083498"
        }
      }
    }
  ]
}

