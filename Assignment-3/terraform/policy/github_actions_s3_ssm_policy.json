{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3UploadAndSSM",
      "Effect": "Allow",
      "Action": ["s3:PutObject","s3:PutObjectAcl"],
      "Resource": "arn:aws:s3:::echeazy-logs-devops-2/*"
    },
    {
      "Sid": "AllowSSMSendCommand",
      "Effect": "Allow",
      "Action": ["ssm:SendCommand","iam:PassRole"],
      "Resource": "*"
    }
  ]
}
